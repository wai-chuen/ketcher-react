{"code":"/****************************************************************************\r\n * Copyright 2021 EPAM Systems\r\n *\r\n * Licensed under the Apache License, Version 2.0 (the \"License\");\r\n * you may not use this file except in compliance with the License.\r\n * You may obtain a copy of the License at\r\n *\r\n *    http://www.apache.org/licenses/LICENSE-2.0\r\n *\r\n * Unless required by applicable law or agreed to in writing, software\r\n * distributed under the License is distributed on an \"AS IS\" BASIS,\r\n * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.\r\n * See the License for the specific language governing permissions and\r\n * limitations under the License.\r\n ***************************************************************************/\r\nimport { Elements, FunctionalGroup } from 'ketcher-core';\r\nclass AttachTool {\r\n    attach;\r\n    editor;\r\n    constructor(editor, attachPoints) {\r\n        this.attach = {\r\n            atomid: attachPoints.atomid || 0,\r\n            bondid: attachPoints.bondid || 0\r\n        };\r\n        this.editor = editor;\r\n        this.editor.selection({\r\n            atoms: [this.attach.atomid],\r\n            bonds: [this.attach.bondid]\r\n        });\r\n    }\r\n    mousemove(event) {\r\n        const rnd = this.editor.render;\r\n        const ci = this.editor.findItem(event, ['atoms', 'bonds']);\r\n        const struct = rnd.ctab.molecule;\r\n        if (ci &&\r\n            ((ci.map === 'atoms' &&\r\n                Elements.get(struct.atoms.get(ci.id)?.label)) ||\r\n                ci.map === 'bonds')) {\r\n            this.editor.hover(ci);\r\n        }\r\n        else {\r\n            this.editor.hover(null);\r\n        }\r\n        return true;\r\n    }\r\n    click(event) {\r\n        const editor = this.editor;\r\n        const rnd = editor.render;\r\n        const molecule = rnd.ctab.molecule;\r\n        const functionalGroups = molecule.functionalGroups;\r\n        const ci = editor.findItem(event, ['atoms', 'bonds']);\r\n        const atomResult = [];\r\n        const bondResult = [];\r\n        const result = [];\r\n        if (ci && functionalGroups.size && ci.map === 'atoms') {\r\n            const atomId = FunctionalGroup.atomsInFunctionalGroup(functionalGroups, ci.id);\r\n            if (atomId !== null) {\r\n                atomResult.push(atomId);\r\n            }\r\n        }\r\n        if (ci && functionalGroups.size && ci.map === 'bonds') {\r\n            const bondId = FunctionalGroup.bondsInFunctionalGroup(molecule, functionalGroups, ci.id);\r\n            if (bondId !== null) {\r\n                bondResult.push(bondId);\r\n            }\r\n        }\r\n        if (atomResult.length > 0) {\r\n            for (const id of atomResult) {\r\n                const fgId = FunctionalGroup.findFunctionalGroupByAtom(functionalGroups, id);\r\n                if (fgId !== null && !result.includes(fgId)) {\r\n                    result.push(fgId);\r\n                }\r\n            }\r\n            this.editor.event.removeFG.dispatch({ fgIds: result });\r\n            return;\r\n        }\r\n        else if (bondResult.length > 0) {\r\n            for (const id of bondResult) {\r\n                const fgId = FunctionalGroup.findFunctionalGroupByBond(molecule, functionalGroups, id);\r\n                if (fgId !== null && !result.includes(fgId)) {\r\n                    result.push(fgId);\r\n                }\r\n            }\r\n            this.editor.event.removeFG.dispatch({ fgIds: result });\r\n            return;\r\n        }\r\n        if (ci &&\r\n            ((ci.map === 'atoms' &&\r\n                Elements.get(molecule.atoms.get(ci.id)?.label)) ||\r\n                ci.map === 'bonds')) {\r\n            if (ci.map === 'atoms') {\r\n                this.attach.atomid = ci.id;\r\n            }\r\n            else\r\n                this.attach.bondid = ci.id;\r\n            this.editor.selection({\r\n                atoms: [this.attach.atomid],\r\n                bonds: [this.attach.bondid]\r\n            });\r\n            this.editor.event.attachEdit.dispatch(this.attach);\r\n        }\r\n        return true;\r\n    }\r\n}\r\nexport default AttachTool;\r\n//# sourceMappingURL=attach.js.map","references":["/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-core/dist/index.d.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/script/editor/Editor.ts"],"map":"{\"version\":3,\"file\":\"attach.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../src/script/editor/tool/attach.ts\"],\"names\":[],\"mappings\":\"AAAA;;;;;;;;;;;;;;6EAc6E;AAE7E,OAAO,EAAE,QAAQ,EAAE,eAAe,EAAE,MAAM,cAAc,CAAA;AAGxD,MAAM,UAAU;IACd,MAAM,CAAK;IACX,MAAM,CAAQ;IAEd,YAAY,MAAM,EAAE,YAAY;QAC9B,IAAI,CAAC,MAAM,GAAG;YACZ,MAAM,EAAE,YAAY,CAAC,MAAM,IAAI,CAAC;YAChC,MAAM,EAAE,YAAY,CAAC,MAAM,IAAI,CAAC;SACjC,CAAA;QACD,IAAI,CAAC,MAAM,GAAG,MAAM,CAAA;QACpB,IAAI,CAAC,MAAM,CAAC,SAAS,CAAC;YACpB,KAAK,EAAE,CAAC,IAAI,CAAC,MAAM,CAAC,MAAM,CAAC;YAC3B,KAAK,EAAE,CAAC,IAAI,CAAC,MAAM,CAAC,MAAM,CAAC;SAC5B,CAAC,CAAA;IACJ,CAAC;IAED,SAAS,CAAC,KAAK;QACb,MAAM,GAAG,GAAG,IAAI,CAAC,MAAM,CAAC,MAAM,CAAA;QAE9B,MAAM,EAAE,GAAG,IAAI,CAAC,MAAM,CAAC,QAAQ,CAAC,KAAK,EAAE,CAAC,OAAO,EAAE,OAAO,CAAC,CAAC,CAAA;QAC1D,MAAM,MAAM,GAAG,GAAG,CAAC,IAAI,CAAC,QAAQ,CAAA;QAChC,IACE,EAAE;YACF,CAAC,CAAC,EAAE,CAAC,GAAG,KAAK,OAAO;gBAClB,QAAQ,CAAC,GAAG,CAAC,MAAM,CAAC,KAAK,CAAC,GAAG,CAAC,EAAE,CAAC,EAAE,CAAC,EAAE,KAAe,CAAC,CAAC;gBACvD,EAAE,CAAC,GAAG,KAAK,OAAO,CAAC,EACrB;YACA,IAAI,CAAC,MAAM,CAAC,KAAK,CAAC,EAAE,CAAC,CAAA;SACtB;aAAM;YACL,IAAI,CAAC,MAAM,CAAC,KAAK,CAAC,IAAI,CAAC,CAAA;SACxB;QACD,OAAO,IAAI,CAAA;IACb,CAAC;IAED,KAAK,CAAC,KAAK;QACT,MAAM,MAAM,GAAG,IAAI,CAAC,MAAM,CAAA;QAC1B,MAAM,GAAG,GAAG,MAAM,CAAC,MAAM,CAAA;QACzB,MAAM,QAAQ,GAAG,GAAG,CAAC,IAAI,CAAC,QAAQ,CAAA;QAClC,MAAM,gBAAgB,GAAG,QAAQ,CAAC,gBAAgB,CAAA;QAClD,MAAM,EAAE,GAAG,MAAM,CAAC,QAAQ,CAAC,KAAK,EAAE,CAAC,OAAO,EAAE,OAAO,CAAC,CAAC,CAAA;QACrD,MAAM,UAAU,GAAkB,EAAE,CAAA;QACpC,MA