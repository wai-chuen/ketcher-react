{"code":"import { Fragment as _Fragment, jsx as _jsx, jsxs as _jsxs } from \"react/jsx-runtime\";\r\n/****************************************************************************\r\n * Copyright 2021 EPAM Systems\r\n *\r\n * Licensed under the Apache License, Version 2.0 (the \"License\");\r\n * you may not use this file except in compliance with the License.\r\n * You may obtain a copy of the License at\r\n *\r\n *    http://www.apache.org/licenses/LICENSE-2.0\r\n *\r\n * Unless required by applicable law or agreed to in writing, software\r\n * distributed under the License is distributed on an \"AS IS\" BASIS,\r\n * WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.\r\n * See the License for the specific language governing permissions and\r\n * limitations under the License.\r\n ***************************************************************************/\r\nimport classes from './ArrowScroll.module.less';\r\nimport clsx from 'clsx';\r\nimport { useEffect, useState } from 'react';\r\nimport { useInterval } from '../../../../../hooks';\r\nconst ArrowScroll = ({ startInView, endInView, scrollUp, scrollDown }) => {\r\n    const [isScrollDown, setScrollDown] = useState(false);\r\n    const [isScrollUp, setScrollUp] = useState(false);\r\n    useInterval(scrollDown, isScrollDown ? 100 : null);\r\n    useInterval(scrollUp, isScrollUp ? 100 : null);\r\n    useEffect(() => {\r\n        return () => {\r\n            setScrollUp(false);\r\n        };\r\n    }, [startInView]);\r\n    useEffect(() => {\r\n        return () => {\r\n            setScrollDown(false);\r\n        };\r\n    }, [endInView]);\r\n    return (_jsxs(\"div\", { className: classes.scroll, children: [endInView ? (_jsx(_Fragment, {})) : (_jsx(\"button\", { onClick: () => scrollDown(), onMouseUp: () => setScrollDown(false), onMouseDown: () => setScrollDown(true), className: clsx(classes.button, classes.down), children: \"\\u25BC\" })), startInView ? (_jsx(_Fragment, {})) : (_jsx(\"button\", { onClick: () => scrollUp(), onMouseUp: () => setScrollUp(false), onMouseDown: () => setScrollUp(true), className: clsx(classes.button, classes.up), children: \"\\u25B2\" }))] }));\r\n};\r\nexport { ArrowScroll };\r\n//# sourceMappingURL=ArrowScroll.js.map","references":["/usr/local/google/home/waich/workspace/ketcher/node_modules/clsx/clsx.d.ts","/usr/local/google/home/waich/workspace/ketcher/node_modules/@types/react/index.d.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/hooks/index.ts"],"map":"{\"version\":3,\"file\":\"ArrowScroll.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../../../src/script/ui/views/toolbars/ArrowScroll/ArrowScroll.tsx\"],\"names\":[],\"mappings\":\";AAAA;;;;;;;;;;;;;;6EAc6E;AAE7E,OAAO,OAAO,MAAM,2BAA2B,CAAA;AAC/C,OAAO,IAAI,MAAM,MAAM,CAAA;AACvB,OAAO,EAAE,SAAS,EAAE,QAAQ,EAAE,MAAM,OAAO,CAAA;AAC3C,OAAO,EAAE,WAAW,EAAE,MAAM,sBAAsB,CAAA;AASlD,MAAM,WAAW,GAAG,CAAC,EACnB,WAAW,EACX,SAAS,EACT,QAAQ,EACR,UAAU,EACO,EAAE,EAAE;IACrB,MAAM,CAAC,YAAY,EAAE,aAAa,CAAC,GAAG,QAAQ,CAAC,KAAK,CAAC,CAAA;IACrD,MAAM,CAAC,UAAU,EAAE,WAAW,CAAC,GAAG,QAAQ,CAAC,KAAK,CAAC,CAAA;IACjD,WAAW,CAAC,UAAU,EAAE,YAAY,CAAC,CAAC,CAAC,GAAG,CAAC,CAAC,CAAC,IAAI,CAAC,CAAA;IAClD,WAAW,CAAC,QAAQ,EAAE,UAAU,CAAC,CAAC,CAAC,GAAG,CAAC,CAAC,CAAC,IAAI,CAAC,CAAA;IAE9C,SAAS,CAAC,GAAG,EAAE;QACb,OAAO,GAAG,EAAE;YACV,WAAW,CAAC,KAAK,CAAC,CAAA;QACpB,CAAC,CAAA;IACH,CAAC,EAAE,CAAC,WAAW,CAAC,CAAC,CAAA;IAEjB,SAAS,CAAC,GAAG,EAAE;QACb,OAAO,GAAG,EAAE;YACV,aAAa,CAAC,KAAK,CAAC,CAAA;QACtB,CAAC,CAAA;IACH,CAAC,EAAE,CAAC,SAAS,CAAC,CAAC,CAAA;IAEf,OAAO,CACL,eAAK,SAAS,EAAE,OAAO,CAAC,MAAM,aAC3B,SAAS,CAAC,CAAC,CAAC,CACX,mBAAK,CACN,CAAC,CAAC,CAAC,CACF,iBACE,OAAO,EAAE,GAAG,EAAE,CAAC,UAAU,EAAE,EAC3B,SAAS,EAAE,GAAG,EAAE,CAAC,aAAa,CAAC,KAAK,CAAC,EACrC,WAAW,EAAE,GAAG,EAAE,CAAC,aAAa,CAAC,IAAI,CAAC,EACtC,SAAS,EAAE,IAAI,CAAC,OAAO,CAAC,MAAM,EAAE,OAAO,CAAC,IAAI,CAAC,uBAGtC,CACV,EACA,WAAW,CAAC,CAAC,CAAC,CACb,mBAAK,CACN,CAAC,CAAC,CAAC,CACF,iBACE,OAAO,EAAE,GAAG,EAAE,CAAC,QAAQ,EAAE,EACzB,SAAS,EAAE,