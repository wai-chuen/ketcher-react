ate, nextContentState, 'insert-characters');\r\n        setEditorState(nextEditorState);\r\n        setShowSpecialSymbols(false);\r\n    };\r\n    return (_jsxs(\"div\", { onBlur: closeSymbolsList, children: [_jsx(\"button\", { title: \"symbols\", onMouseDown: (e) => {\r\n                    e.preventDefault();\r\n                    setShowSpecialSymbols(!showSpecialSymbols);\r\n                }, className: showSpecialSymbols ? classes.activeTextButton : classes.textButton, children: _jsx(Icon, { name: \"text-special-symbols\" }) }), showSpecialSymbols && _jsx(SpecialSymbolsList, { select: addSymbol })] }));\r\n};\r\nexport { SpecialSymbolsButton };\r\n//# sourceMappingURL=SpecialSymbolsButton.js.map","references":["/usr/local/google/home/waich/workspace/ketcher/node_modules/@types/draft-js/index.d.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/script/ui/component/view/icon.jsx","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/script/ui/views/modal/components/Text/SpecialSymbolsList/SpecialSymbolsList.tsx","/usr/local/google/home/waich/workspace/ketcher/node_modules/@types/react/index.d.ts"],"map":"{\"version\":3,\"file\":\"SpecialSymbolsButton.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../../../../../src/script/ui/views/modal/components/Text/SpecialSymbols/SpecialSymbolsButton.tsx\"],\"names\":[],\"mappings\":\";AAAA;;;;;;;;;;;;;;6EAc6E;AAE7E,OAAO,EAAE,WAAW,EAAE,QAAQ,EAAE,MAAM,UAAU,CAAA;AAEhD,OAAO,IAAI,MAAM,oCAAoC,CAAA;AACrD,OAAO,EAAE,kBAAkB,EAAE,MAAM,0CAA0C,CAAA;AAC7E,OAAO,OAAO,MAAM,oCAAoC,CAAA;AACxD,OAAO,EAAE,QAAQ,EAAE,MAAM,OAAO,CAAA;AAEhC,MAAM,oBAAoB,GAAG,CAAC,EAAE,WAAW,EAAE,cAAc,EAAE,MAAM,EAAE,EAAE,EAAE;IACvE,MAAM,CAAC,kBAAkB,EAAE,qBAAqB,CAAC,GAAG,QAAQ,CAAC,KAAK,CAAC,CAAA;IAEnE,MAAM,WAAW,GAAG,CAAC,KAAK,EAAE,EAAE;QAC5B,KAAK,CAAC,eAAe,EAAE,CAAA;QACvB,KAAK,CAAC,cAAc,EAAE,CAAA;QACtB,qBAAqB,CAAC,KAAK,CAAC,CAAA;IAC9B,CAAC,CAAA;IAED,MAAM,gBAAgB,GAAG,CAAC,KAAK,EAAE,EAAE;QACjC,IAAI,CAAC,KAAK,CAAC