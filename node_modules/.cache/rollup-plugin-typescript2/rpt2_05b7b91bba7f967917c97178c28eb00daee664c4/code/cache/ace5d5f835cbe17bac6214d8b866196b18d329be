tem({ id, options, ...rest });\r\n    const scrollUp = () => {\r\n        scrollRef.current.scrollTop -= sizeRef.current.offsetHeight;\r\n    };\r\n    const scrollDown = () => {\r\n        scrollRef.current.scrollTop += sizeRef.current.offsetHeight;\r\n    };\r\n    const status = rest.status;\r\n    const Group = ({ items, className }) => {\r\n        const visibleItems = [];\r\n        if (items) {\r\n            items.forEach((item) => {\r\n                let visible = true;\r\n                if (status[item.id]?.hidden) {\r\n                    visible = false;\r\n                }\r\n                else if (item.options?.every((option) => status[option.id]?.hidden)) {\r\n                    visible = false;\r\n                }\r\n                if (visible)\r\n                    visibleItems.push(item);\r\n            });\r\n        }\r\n        return visibleItems.length ? (_jsx(\"div\", { className: clsx(classes.group, className), children: visibleItems.map((item) => {\r\n                switch (item.id) {\r\n                    case 'bond-common':\r\n                        return _createElement(Bond, { ...rest, height: height, key: item.id });\r\n                    case 'transform-rotate':\r\n                        return _createElement(Transform, { ...rest, height: height, key: item.id });\r\n                    case 'rgroup':\r\n                        return _createElement(RGroup, { ...rest, key: item.id });\r\n                    case 'shapes':\r\n                        return _createElement(Shape, { ...rest, key: item.id });\r\n                    default:\r\n                        return _jsx(Item, { id: item.id, options: item.options }, item.id);\r\n                }\r\n            }) })) : null;\r\n    };\r\n    return (_jsxs(\"div\", { className: clsx(classes.root, className), ref: ref, children: [_jsxs(\"div\", { className: classes.buttons, ref: scrollRef, children: [_jsx(\"div\", { className: classes.listener, ref: startRef, children: _jsx(Group, { className: classes.groupItem, items: [\r\n                                { id: 'hand' },\r\n                                { id: 'select', options: selectOptions },\r\n                                { id: 'erase' }\r\n                            ] }) }), _jsx(Group, { className: classes.groupItem, items: [\r\n                            {\r\n                                id: 'bonds',\r\n                                options: [\r\n                                    ...bondCommon,\r\n                                    ...bondQuery,\r\n                                    ...bondSpecial,\r\n                                    ...bondStereo\r\n                                ]\r\n                            },\r\n                            { id: 'chain' }\r\n                        ] }), _jsx(Group, { className: classes.groupItem, items: [{ id: 'enhanced-stereo' }] }), _jsx(Group, { className: classes.groupItem, items: [{ id: 'charge-plus' }, { id: 'charge-minus' }] }), _jsx(Group, { className: classes.groupItem, items: [\r\n                            {\r\n                                id: 'transforms',\r\n                                options: transformOptions\r\n                            }\r\n                        ] }), _jsx(Group, { className: classes.groupItem, items: [{ id: 'sgroup' }] }), _jsx(Group, { className: classes.groupItem, items: [\r\n                            { id: 'reaction-plus' },\r\n                            { id: 'arrows', options: arrowsOptions },\r\n                            {\r\n                                id: 'reaction-mapping-tools',\r\n                                options: mappingOptions\r\n                            }\r\n                        ] }), _jsx(\"div\", { className: classes.listener, ref: sizeRef, children: _jsx(Group, { className: classes.groupItem, items: [{ id: 'rgroup', options: rGroupOptions }] }) }), _jsx(Group, { className: classes.groupItem, items: [{ id: 'shapes', options: shapeOptions }] }), _jsx(\"div\", { ref: endRef, children: _jsx(Group, { className: classes.groupItem, items: [{ id: 'text' }] }) })] }), _jsx(ArrowScroll, { startInView: startInView, endInView: endInView, scrollUp: scrollUp, scrollDown: scrollDown })] }));\r\n};\r\nexport { LeftToolbar };\r\n//# sourceMappingURL=LeftToolbar.js.map","references":["/usr/local/google/home/waich/workspace/ketcher/node_modules/@types/react/index.d.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/script/ui/views/toolbars/ToolbarGroupItem/index.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/script/ui/views/toolbars/toolbar.types.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/script/ui/views/toolbars/LeftToolbar/leftToolbarOptions.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/script/ui/views/toolbars/ArrowScroll/index.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/script/ui/views/toolbars/LeftToolbar/Bond/index.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/script/ui/views/toolbars/LeftToolbar/RGroup/index.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/script/ui/views/toolbars/LeftToolbar/Shape/index.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/script/ui/views/toolbars/LeftToolbar/Transform/index.ts","/usr/local/google/home/waich/workspace/ketcher/node_modules/clsx/clsx.d.ts","/usr/local/google/home/waich/workspace/ketcher/node_modules/react-intersection-observer/index.d.ts","/usr/local/google/home/waich/workspace/ketcher/packages/ketcher-react/src/hooks/index.ts"],"map":"{\"version\":3,\"file\":\"LeftToolbar.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../../../src/script/ui/views/toolbars/LeftToolbar/LeftToolbar.tsx\"],\"names\":[],\"mappings\":\";;AAAA;;;;;;;;;;;;;;6EAc6E;AAE7E,OAAO,EAAwB,MAAM,EAAE,MAAM,OAAO,CAAA;AACpD,OAAO,E